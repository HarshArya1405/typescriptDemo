{
  "name": "demotype",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.ts",
    "prepare": "husky install",
    "lint": "eslint src",
    "precommit": "npm run lint",
    "typeorm": "typeorm-ts-node-esm -d ./src/loaders/typeormLoader.ts",
    "migration:create": "typeorm-ts-node-esm migration:create ./src/api/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm migration:generate ./src/api/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "aws-sdk": "^2.1549.0",
    "axios": "^1.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-openid-connect": "^2.17.1",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "microframework-w3tec": "^0.6.3",
    "mixpanel": "^0.18.0",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "sequelize": "^6.36.0",
    "sequelize-typescript": "^2.1.6",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-typedi-extensions": "^0.4.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache"
  },
  "husky": {
